{
    "parser": "babel-eslint",
    "extends": ["standard", "standard-react", "prettier", "prettier/react", "prettier/standard"],
    "env": {
        "es6": true
    },
    "plugins": ["react"],
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "space-before-function-paren": 0,
        "react/jsx-boolean-value": 0,
        "react/prop-types": 0
    }
}
